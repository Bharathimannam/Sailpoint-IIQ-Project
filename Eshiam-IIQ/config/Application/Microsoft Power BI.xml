    <Map>
      <entry key="SQL" value="SELECT EMP_ID, EMAIL_ADDRESS, USER_NAME, ACCESS_TYPE, MASTER_DELETE_FLAG, COMPANY_NO, REST_NUMBER, CHAIN_NUMBER, AREA_NUMBER, DELETE_FLAG, ENTRY_FLAG, PETTY_CASH_ENTRY, SALES_FLAG, PL_FLAG, Marketing_Flag, PAYROLL_FLAG, ARCHIVING_FLAG, USER_TITLE, USER_ID, COST_CENTER, USER_STATUS, &#xD;&#xA;CASE&#xD;&#xA;    WHEN SALES_FLAG = &apos;T&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : Sales&apos;)&#xD;&#xA;END AS SALES_GROUP,&#xD;&#xA;CASE&#xD;&#xA;    WHEN PL_FLAG = &apos;T&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : PL&apos;)&#xD;&#xA;END AS PL_GROUP,&#xD;&#xA;CASE&#xD;&#xA;    WHEN Marketing_Flag = &apos;T&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : Marketing&apos;)&#xD;&#xA;END AS MARKETING_GROUP,&#xD;&#xA;CASE&#xD;&#xA;    WHEN SALES_FLAG = &apos;T&apos; and PL_FLAG = &apos;F&apos; and Marketing_Flag = &apos;F&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : Sales&apos;)&#xD;&#xA;END AS STORE_INFO_S,&#xD;&#xA;CASE&#xD;&#xA;    WHEN SALES_FLAG = &apos;F&apos; and PL_FLAG = &apos;T&apos; and Marketing_Flag = &apos;F&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : P/L&apos;)&#xD;&#xA;END AS STORE_INFO_L,&#xD;&#xA;CASE&#xD;&#xA;    WHEN SALES_FLAG = &apos;F&apos; and PL_FLAG = &apos;F&apos; and Marketing_Flag = &apos;T&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : Marketing&apos;)&#xD;&#xA;END AS STORE_INFO_M,&#xD;&#xA;CASE&#xD;&#xA;    WHEN SALES_FLAG = &apos;F&apos; and PL_FLAG = &apos;T&apos; and Marketing_Flag = &apos;T&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : P/L,Marketing&apos;)&#xD;&#xA;END AS STORE_INFO_LM,&#xD;&#xA;CASE&#xD;&#xA;    WHEN SALES_FLAG = &apos;T&apos; and PL_FLAG = &apos;F&apos; and Marketing_Flag = &apos;T&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : Sales,Marketing&apos;)&#xD;&#xA;END AS STORE_INFO_SM,&#xD;&#xA;CASE&#xD;&#xA;    WHEN SALES_FLAG = &apos;T&apos; and PL_FLAG = &apos;T&apos; and Marketing_Flag = &apos;F&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : Sales,P/L&apos;)&#xD;&#xA;END AS STORE_INFO_SL,&#xD;&#xA;CASE&#xD;&#xA;    WHEN SALES_FLAG = &apos;T&apos; and PL_FLAG = &apos;T&apos; and Marketing_Flag = &apos;T&apos; THEN concat(r.COUNTRY_DESC,&apos; : &apos;,r.CHAIN_NAME,&apos; : &apos;,r.[Restaurant Name],&apos; : Sales,P/L,Marketing&apos;)&#xD;&#xA;END AS STORE_INFO_SLM&#xD;&#xA;FROM USERDB.User_Security s&#xD;&#xA;INNER JOIN USERDB.DimRestaurant r ON s.COMPANY_NO = r.COMPANY_ID and s.REST_NUMBER = r.[Restaurant Code]&#xD;&#xA;WHERE s.EMAIL_ADDRESS IS NOT NULL&#xD;&#xA;ORDER BY s.EMAIL_ADDRESS;"/>
